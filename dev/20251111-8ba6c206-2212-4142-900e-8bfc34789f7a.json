    "/hello": {
      "get": {
        "tags": [
          "Hello"
        ],
        "summary": "인사 메시지 반환",
        "description": "Hello Spring Boot! 메시지를 반환합니다.",
        "operationId": "hello",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }

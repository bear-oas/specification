  "paths": {
    "/api/users/{id}": {
      "get": {
        "tags": [
          "User"
        ],
        "summary": "사용자 ID로 조회",
        "description": "특정 ID를 가진 사용자 정보를 반환합니다.",
        "operationId": "getUserById",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "사용자 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            },
            "example": 1
          }
        ],
        "responses": {
          "200": {
            "description": "사용자를 찾음",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "404": {
            "description": "사용자를 찾을 수 없음"
          }
        }
      },
      "put": {
        "tags": [
          "User"
        ],
        "summary": "사용자 정보 수정",
        "description": "기존 사용자의 정보를 업데이트합니다.",
        "operationId": "updateUser",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "사용자 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            },
            "example": 1
          }
        ],
        "requestBody": {
          "description": "수정할 사용자 정보",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "사용자 정보가 성공적으로 수정됨",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "404": {
            "description": "사용자를 찾을 수 없음"
          }

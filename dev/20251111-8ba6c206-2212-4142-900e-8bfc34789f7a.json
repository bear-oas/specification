      },
      "delete": {
        "tags": [
          "User"
        ],
        "summary": "사용자 삭제",
        "description": "특정 ID를 가진 사용자를 시스템에서 삭제합니다.",
        "operationId": "deleteUser",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "사용자 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            },
            "example": 1
          }
        ],
        "responses": {
          "204": {
            "description": "사용자가 성공적으로 삭제됨"
          },
          "404": {
            "description": "사용자를 찾을 수 없음"
          }
        }
      }
